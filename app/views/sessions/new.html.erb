<%= render '/shared/content_title', title: 'User Log In'%>
<hr>
<div class="well">
  <%= form_tag '/login' do |f| %>
  <div class="form-group">
    <%= label_tag :username %>
    <%= text_field_tag :username, params[:username] || '' %>
  </div>
  <div class="form-group">
    <%= label_tag :password %>
    <%= password_field_tag :password, params[:password] || '' %>
  </div>
  <br>
  <%= submit_tag "Login", class: "btn btn-success" %>
  <% end %>
</div>
